extern   double          floor          (double  __x)    __attribute__ ((__nothrow__ , __leaf__ ))   __attribute__ (    (__const__)  ); extern   double         __floor          (double  __x)    __attribute__ ((__nothrow__ , __leaf__ ))   __attribute__ (    (__const__)  )  ;

