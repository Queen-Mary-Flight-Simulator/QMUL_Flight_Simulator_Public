void DrawAlphanumericBox(int x, int y, char str[]);
void DrawQuestionBox(int x, int y, char str[]);
void DrawButtonsBox(int x, int y, char str[]);
void DrawFlightDataBox(int x, int y, char str[]);
void DrawErrorBox(int x, int y);
